{
  "name": "patkars-realty",
  "version": "1.0.0",
  "description": "Patkar's Realty - Full Stack Real Estate Application",
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "real-estate",
    "fullstack",
    "react",
    "nodejs"
  ],
  "author": "Patkar's Realty",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.3.1"
  }
}
